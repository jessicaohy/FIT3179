{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 500,
  "height": 400,
  "title": "COVID19 New Deaths (2020) according to Median Age",
  "data": {
    "url": "https://raw.githubusercontent.com/jessicaohy/FIT3179/master/Data/cleanedCovidWorld.csv"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "median_age",
      "type": "quantitative",
      "title": "Median Age",
      "axis": {"tickCount": 4}
    },
    "y": {"field": "new_deaths", "type": "quantitative", "title": "Deaths", "axis": {"tickCount": 7}, "scale": {"domain": [0, 220000]}},
    "color": {"value": "#02A3A6"}
  },
  "config": {}
}
