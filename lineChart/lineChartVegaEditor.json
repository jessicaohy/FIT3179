{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 300,
  "height": 300,
  "title": "COVID19 (2020)",
  "data": {
    "url": "https://raw.githubusercontent.com/jessicaohy/FIT3179/master/Data/cleanedMonthCovidWorld.csv"
  },
  "mark": {"type": "line", "point": true},
  "encoding": {
    "x": {"field": "date"},
    "y": {"field": "new_cases", "type": "quantitative"},
    "color": {
      "field": "location",
      "type": "nominal",
      "scale": {"domain": ["United States of America", "Brazil", "India", "China", "Australia"], "range": ["#66CAF2", "#1F3E99", "#02A3A6", "#96CC92", "#8538A6"]}
}
  },
  "config": {}
}
